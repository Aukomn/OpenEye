<?xml version="1.0" encoding="utf-8"?>
<com.scwang.smart.refresh.layout.SmartRefreshLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/refreshday"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <com.scwang.smart.refresh.header.MaterialHeader
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <androidx.recyclerview.widget.RecyclerView xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/listdaily"
        android:name="com.aukomn.myopeneye.ui.home.ui.main.DailyFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        app:layoutManager="LinearLayoutManager"
        tools:context=".ui.home.ui.main.DailyFragment"
        tools:listitem="@layout/item_daily" />
    <com.scwang.smart.refresh.footer.ClassicsFooter
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</com.scwang.smart.refresh.layout.SmartRefreshLayout>
